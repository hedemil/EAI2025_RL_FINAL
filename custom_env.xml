<mujoco model="go1 feet only rough terrain scene">
  <include file="go1_mjx_feetonly.xml"/>

  <statistic center="0 0 0.1" extent="0.8" meansize="0.04"/>

  <visual>
    <rgba force="1 0 0 1"/>
    <global azimuth="120" elevation="-20"/>
    <map force="0.01"/>
    <scale forcewidth="0.3" contactwidth="0.5" contactheight="0.2"/>
    <quality shadowsize="8192"/>
  </visual>

  <asset>
    <texture type="skybox" builtin="gradient" rgb1="1 1 1" rgb2="1 1 1" width="800" height="800"/>
    <texture type="2d" name="groundplane" builtin="checker" mark="edge" rgb1="1 1 1" rgb2="1 1 1" markrgb="0 0 0"
      width="300" height="300"/>
    <material name="groundplane" texture="groundplane" texuniform="true" texrepeat="5 5" reflectance="0"/>
  </asset>

  <worldbody>
    <geom name="floor" size="0 0 0.01" type="plane" material="groundplane" contype="1" conaffinity="0" priority="1"
      friction="1.0 0.005 0.0001" condim="3"/>
    <!-- Walls as mocap bodies (kinematic, not affected by physics) -->
    <!-- Wall 1: Forward blocking path -->
    <body name="wallbody1" pos="1.5 0 0.05" mocap="true">
      <geom name="wallgeom_1" contype="1" conaffinity="0" friction="1.0 0.005 0.0001" rgba="0.6 0.6 0.6 1" type="box" size="0.3 10.0 0.05"/>
    </body>

    <!-- Wall 2: Backward blocking path -->
    <body name="wallbody_2" pos="-1.5 0 0.04" mocap="true">
      <geom name="wallgeom_2" contype="1" conaffinity="0" friction="1.0 0.005 0.0001" rgba="0.6 0.6 0.6 1" type="box" size="0.3 10.0 0.05"/>
    </body>

    <!-- Wall 3: Left side blocking -->
    <body name="wallbody_3" pos="0 3.0 0.06" mocap="true">
      <geom name="wallgeom_3" contype="1" conaffinity="0" friction="1.0 0.005 0.0001" rgba="0.6 0.6 0.6 1" type="box" size="10.0 0.3 0.06"/>
    </body>

    <!-- Wall 4: Right side blocking -->
    <body name="wallbody_4" pos="0 -3.0 0.045" mocap="true">
      <geom name="wallgeom_4" contype="1" conaffinity="0" friction="1.0 0.005 0.0001" rgba="0.6 0.6 0.6 1" type="box" size="10.0 0.3 0.045"/>
    </body>

    <!-- Wall 5: Additional crossing wall -->
    <body name="wallbody_5" pos="3.5 0 0.05" mocap="true">
      <geom name="wallgeom_5" contype="1" conaffinity="0" friction="1.0 0.005 0.0001" rgba="0.6 0.6 0.6 1" type="box" size="0.2 8.0 0.05"/>
    </body>

    <!-- Wall 6: Additional crossing wall -->
    <body name="wallbody_6" pos="-3.5 0 0.035" mocap="true">
      <geom name="wallgeom_6" contype="1" conaffinity="0" friction="1.0 0.005 0.0001" rgba="0.6 0.6 0.6 1" type="box" size="0.2 8.0 0.035"/>
    </body>
  </worldbody>


  <!-- <include file="sensor_feet.xml"/> -->
  <!-- <include file="sensor_knees.xml"/> -->
  <!-- <include file="sensor_fullcollision.xml"/> -->

  <keyframe>
    <key name="home" qpos="
    0 0 0.278
    1 0 0 0
    0.1 0.9 -1.8
    -0.1 0.9 -1.8
    0.1 0.9 -1.8
    -0.1 0.9 -1.8"
      ctrl="0.1 0.9 -1.8 -0.1 0.9 -1.8 0.1 0.9 -1.8 -0.1 0.9 -1.8"/>
    <key name="home_higher" qpos="0 0 0.35 1 0 0 0 0 0.82 -1.63 0 0.82 -1.63 0 0.82 -1.63 0 0.82 -1.63"
      ctrl="0 0.82 -1.63 0 0.82 -1.63 0 0.82 -1.63 0 0.82 -1.63"/>
  </keyframe>
</mujoco>